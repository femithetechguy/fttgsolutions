{
  "buildCommand": "mkdir -p .vercel/output/static && cp -r public/* .vercel/output/static/",
  "devCommand": "python3 -m http.server 3000",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
